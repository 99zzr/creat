      "surfaces": {
        "in_app_panel": {
          "name": "使用剪贴蒙版制作毛刺艺术效果",
          "version": "2.0.1",
          "image": "https://cc-cdn.adobe.com/graceland/PHSP/en_US/2.0.0/create-a-glitch-art-effect-with-a-clipping-mask/glitch_art_effect_with_clipping_mask-HERO.jpg?1630621071816",
          "content": [
            {
              "description": "要准备制作毛刺效果的文件，您需要复制“主体”图层并调整其在“图层”面板中的位置。",
              "key": "photoshop/how-to/in-app/create-a-glitch-art-effect-with-a-clipping-mask-step-2",
              "title": "复制并调整“主体”图层的位置",
              "taskList": {
                "tasks": [
                  {
                    "key": "photoshop/how-to/in-app/create-a-glitch-art-effect-with-a-clipping-mask-step-2-task-1",
                    "coachmark": {
                      "content": "单击“图案”图层缩览图左侧的空白小方块，以显示此图层。",
                      "position": {
                        "type": "view",
                        "arrow": "top-left",
                        "arrowOrientation": "horizontal",
                        "panel": "layers",
                        "viewID": "layr"
                      },
                      "type": "text",
                      "key": "coachmark-1"
                    },
                    "successConditions": [],
                    "type": "Step"
                  },
                  {
                    "key": "photoshop/how-to/in-app/create-a-glitch-art-effect-with-a-clipping-mask-step-2-task-2",
                    "coachmark": {
                      "content": "“图案”图层中的白色条纹周围都是透明区域。这是生成毛刺效果的前提。单击“{NEXT}”以继续。",
                      "position": {
                        "type": "canvas",
                        "arrow": "top-left",
                        "arrowOrientation": "horizontal",
                        "offset": {
                          "left": "62%",
                          "top": "63%"
                        }
                      },
                      "type": "text",
                      "key": "coachmark-2"
                    },
                    "successConditions": [],
                    "type": "Step"
                  },
                  {
                    "key": "photoshop/how-to/in-app/create-a-glitch-art-effect-with-a-clipping-mask-step-2-task-3",
                    "coachmark": {
                      "content": "选择“主体”图层。",
                      "position": {
                        "type": "appmenu",
                        "name": "layer"
                      },
                      "type": "text",
                      "key": "coachmark-3"
                    },
                    "successConditions": [
                      {
                        "event": "invokeCommand",
                        "descriptor": {
                          "commandID": 1135,
                          "_isCommand": false,
                          "kcanDispatchWhileModal": true
                        }
                      }
                    ],
                    "type": "Step"
                  },
                  {
                    "key": "photoshop/how-to/in-app/create-a-glitch-art-effect-with-a-clipping-mask-step-2-task-4",
                    "coachmark": {
                      "content": "选择“{2970}”",
                      "position": {
                        "type": "dialog"
                      },
                      "type": "text",
                      "key": "coachmark-4"
                    },
                    "successConditions": [],
                    "type": "Step"
                  },
                  {
                    "key": "photoshop/how-to/in-app/create-a-glitch-art-effect-with-a-clipping-mask-step-2-task-5",
                    "coachmark": {
                      "content": "向上拖动“主体”拷贝图层以调整其位置，使其位于“图案”图层的上方。",
                      "position": {
                        "type": "view",
                        "arrow": "top-left",
                        "arrowOrientation": "horizontal",
                        "panel": "layers",
                        "viewID": "layr"
                      },
                      "type": "text",
                      "key": "coachmark-5"
                    },
                    "successConditions": [
                      {
                        "event": "move",
                        "descriptor": {
                          "null": {
                            "_enum": "ordinal",
                            "_ref": "layer",
                            "_value": "targetEnum"
                          },
                          "to": {
                            "_index": 3,
                            "_ref": "layer"
                          }
                        }
                      }
                    ],
                    "type": "Step"
                  }
                ]
              }
            },
            {
              "description": "剪贴蒙版将使用下方图层的像素和透明区域来确定上方图层的可见内容。",
              "key": "photoshop/how-to/in-app/create-a-glitch-art-effect-with-a-clipping-mask-step-3",
              "title": "添加剪贴蒙版以创建毛刺效果",
              "taskList": {
                "tasks": [
                  {
                    "key": "photoshop/how-to/in-app/create-a-glitch-art-effect-with-a-clipping-mask-step-3-task-1",
                    "coachmark": {
                      "content": "按住 Opt 键 (Mac) 或 Alt 键 (Win) 并单击“主体”拷贝图层和“图案”图层之间的边框，以创建剪贴蒙版。单击“{NEXT}”以继续。",
                      "position": {
                        "type": "view",
                        "arrow": "top-left",
                        "arrowOrientation": "horizontal",
                        "panel": "layers",
                        "viewID": "layr"
                      },
                      "type": "text",
                      "key": "coachmark-1"
                    },
                    "successConditions": [],
                    "type": "Step"
                  },
                  {
                    "key": "photoshop/how-to/in-app/create-a-glitch-art-effect-with-a-clipping-mask-step-3-task-2",
                    "coachmark": {
                      "content": "“主体”拷贝图层上的向下箭头表示此图层已剪切到下面的“图案”图层，生成剪贴蒙版。单击“{NEXT}”以继续。",
                      "position": {
                        "type": "view",
                        "arrow": "top-left",
                        "arrowOrientation": "horizontal",
                        "panel": "layers",
                        "viewID": "layr"
                      },
                      "type": "text",
                      "key": "coachmark-2"
                    },
                    "successConditions": [],
                    "type": "Step"
                  },
                  {
                    "key": "photoshop/how-to/in-app/create-a-glitch-art-effect-with-a-clipping-mask-step-3-task-3",
                    "coachmark": {
                      "content": "选择“{moveTool}”。如果此工具已处于选中状态，请选择其他工具，然后重新单击此工具。",
                      "position": {
                        "type": "tool",
                        "id": "moveTool"
                      },
                      "type": "text",
                      "key": "coachmark-3"
                    },
                    "successConditions": [
                      {
                        "event": "select",
                        "descriptor": {
                          "null": {
                            "_ref": "moveTool"
                          }
                        }
                      }
                    ],
                    "type": "Step"
                  },
                  {
                    "key": "photoshop/how-to/in-app/create-a-glitch-art-effect-with-a-clipping-mask-step-3-task-4",
                    "coachmark": {
                      "content": "向左拖动“主体”拷贝图层以查看毛刺效果。剪贴蒙版由“图案”图层上的透明区域所形成，这些透明区域会隐藏上一图层的相应区域。单击“{NEXT}”以继续。",
                      "position": {
                        "type": "canvas",
                        "arrow": "top-left",
                        "arrowOrientation": "horizontal",
                        "offset": {
                          "left": "68%",
                          "top": "28%"
                        }
                      },
                      "type": "text",
                      "key": "coachmark-4"
                    },
                    "successConditions": [],
                    "type": "Step"
                  },
                  {
                    "key": "photoshop/how-to/in-app/create-a-glitch-art-effect-with-a-clipping-mask-step-3-task-5",
                    "coachmark": {
                      "content": "选择“图案”图层，使其处于现用状态。",
                      "position": {
                        "type": "layer",
                        "name": "Pattern"
                      },
                      "type": "text",
                      "key": "coachmark-5"
                    },
                    "successConditions": [
                      {
                        "event": "select",
                        "descriptor": {
                          "null": {
                            "_name": "Pattern",
                            "_ref": "layer"
                          }
                        }
                      }
                    ],
                    "type": "Step"
                  },
                  {
                    "key": "photoshop/how-to/in-app/create-a-glitch-art-effect-with-a-clipping-mask-step-3-task-6",
                    "coachmark": {
                      "content": "拖动即可移动“图案”图层。这是另一种修改毛刺效果的方法。尝试变换各个图层的位置，找到您喜欢的图层组合。单击“{NEXT}”以继续。",
                      "position": {
                        "type": "canvas",
                        "arrow": "top-left",
                        "arrowOrientation": "horizontal",
                        "offset": {
                          "left": "68%",
                          "top": "59%"
                        }
                      },
                      "type": "text",
                      "key": "coachmark-6"
                    },
                    "successConditions": [],
                    "type": "Step"
                  }
                ]
              }
            },
            {
              "key": "photoshop/how-to/in-app/create-a-glitch-art-effect-with-a-clipping-mask-step-4",
              "title": "大功告成！向“主体”拷贝图层添加剪贴蒙版之后，该图层的可见区域取决于下方的“图案”图层，从而产生毛刺效果。",
              "fallbackTitle": "大功告成！向“主体”拷贝图层添加剪贴蒙版之后，该图层的可见区域取决于下方的“图案”图层，从而产生毛刺效果。",
              "credit": "Adobe Stock：Parad St"
            }
